<connectorMetaData path="mongo">
	<name>MongoDB Writer</name>
	<author>Davut Gurbuz</author>
	<pluginVersion>@mirthversion</pluginVersion>
	<mirthVersion>3.9.0</mirthVersion>
	<url>https://www.cloudsolution-sa.com/</url>
	<description>This connector allows CS Connect to insert JSON documents to mongo DB</description>
	<clientClassName>com.mirth.connect.connectors.mongo.MongoWriter</clientClassName>
	<serverClassName>com.mirth.connect.connectors.mongo.MongoDispatcher</serverClassName>
	<sharedClassName>com.mirth.connect.connectors.mongo.MongoDispatcherProperties</sharedClassName>
	<library type="CLIENT" path="mongo-client.jar" />
	<library type="SHARED" path="mongo-shared.jar" />
	<library type="SERVER" path="mongo-server.jar" />
	<library type="SERVER" path="lib/mongo-java-driver-3.8.1.jar" />
	<library type="SERVER" path="lib/spring-data-mongodb-1.10.4.RELEASE.jar" />
	<apiProvider type="SERVER_CLASS" name="com.mirth.connect.connectors.mongo.MongoConnectorServlet"/>
	<transformers></transformers>
	<protocol>mongo</protocol>
	<type>DESTINATION</type>
</connectorMetaData>